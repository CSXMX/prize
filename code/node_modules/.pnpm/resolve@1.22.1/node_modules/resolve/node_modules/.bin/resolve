#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lichangqin/Desktop/前端学习/学习笔记/vite小册/rollup/rollup_config/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lichangqin/Desktop/前端学习/学习笔记/vite小册/rollup/rollup_config/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/resolve" "$@"
else
  exec node  "$basedir/../../bin/resolve" "$@"
fi
